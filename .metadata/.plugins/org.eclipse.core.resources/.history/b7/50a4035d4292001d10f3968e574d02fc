
public class ForTest1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int dan = 2;
		for(int i=1; i<=9; i++) {
			System.out.println(dan+" x "+i+" = "+(dan*i));
		}
		
		System.out.println("$$$$$$$$$$$$$$$");
		int dan2 = 3;
		int i =1;
		while(i<10) {
			System.out.println(dan2+" x "+i+" = "+(dan*i));
			i++;
		}
	}

}
